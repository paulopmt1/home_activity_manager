        <a href="{{ hwi_oauth_login_url('google') }}" class="btn btn-block btn-social btn-google" onclick="ga('aurynTracker.send', 'event', {eventCategory: '_trackEvent', eventAction: 'click', eventLabel: 'btn-google', transport: 'beacon'});">
